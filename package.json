{
	"name": "food-friend-server",
	"version": "1.0.0",
	"main": "src/index.ts",
	"repository": "https://github.com/lednhatkhanh/food-friend-server.git",
	"author": "VTeamDev",
	"license": "MIT",
	"private": false,
	"engines": {
		"node": ">=8.9.0",
		"yarn": ">=1.3.2"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=production node build/index.js",
		"dev": "cross-env NODE_ENV=development nodemon",
		"build": "cross-env NODE_ENV=production tsc",
		"clean": "rimraf build",
		"type-check": "tslint --project tsconfig.json --config tslint.json 'src/**/*.ts'",
		"precommit": "lint-staged",
		"codegen":
			"apollo-codegen introspect-schema http://localhost:8080/graphql --output schema.json",
		"docgen": "graphdoc"
	},
	"lint-staged": {
		"src/**/*.ts": ["type-check", "git add"]
	},
	"graphdoc": {
		"endpoint": "http://localhost:8080/graphql",
		"output": "./docs"
	},
	"dependencies": {
		"apollo-link": "^1.0.7",
		"apollo-server-express": "^1.3.2",
		"bcrypt": "^1.0.3",
		"body-parser": "^1.18.2",
		"class-transformer": "^0.1.8",
		"class-validator": "^0.7.3",
		"cors": "^2.8.4",
		"dotenv": "^4.0.0",
		"express": "^4.16.2",
		"graphql": "0.11.7",
		"graphql-date": "^1.0.3",
		"graphql-tools": "^2.14.1",
		"helmet": "^3.9.0",
		"jsonwebtoken": "^8.1.0",
		"pg": "^7.4.0",
		"reflect-metadata": "^0.1.10",
		"typeorm": "^0.2.0-alpha.13",
		"winston": "^2.4.0"
	},
	"devDependencies": {
		"@2fd/graphdoc": "^2.4.0",
		"@types/bcrypt": "^1.0.0",
		"@types/cors": "^2.8.3",
		"@types/dotenv": "^4.0.2",
		"@types/express": "^4.11.0",
		"@types/graphql": "^0.11.7",
		"@types/graphql-date": "^1.0.2",
		"@types/helmet": "^0.0.37",
		"@types/jsonwebtoken": "^7.2.5",
		"@types/node": "^8.5.2",
		"@types/winston": "^2.3.7",
		"apollo-codegen": "^0.18.3",
		"cross-env": "^5.1.3",
		"husky": "^0.14.3",
		"lint-staged": "^6.0.0",
		"nodemon": "^1.14.3",
		"prettier": "^1.9.2",
		"rimraf": "^2.6.2",
		"ts-node": "^4.1.0",
		"tslib": "^1.8.1",
		"tslint": "^5.8.0",
		"tslint-config-prettier": "^1.6.0",
		"tslint-plugin-prettier": "^1.3.0",
		"typescript": "^2.6.2"
	}
}
